{
	"$schema": "../gen/schemas/macOS-schema.json",
	"identifier": "migrated",
	"description": "permissions that were migrated from v1",
	"local": true,
	"windows": ["main"],
	"permissions": [
		"path:default",
		"event:default",
		"window:default",
		"app:default",
		"resources:default",
		"menu:default",
		"tray:default",
		"fs:allow-read-file",
		"fs:allow-remove",
		"fs:allow-exists",
		"fs:scope-app-recursive",
		"window:allow-maximize",
		"window:allow-unmaximize",
		"window:allow-start-dragging",
		"shell:allow-open",
		"dialog:allow-open",
		{
			"identifier": "http:default",
			"allow": [
				{
					"url": "https://**/"
				}
			]
		},
		"os:allow-platform",
		"os:allow-version",
		"os:allow-os-type",
		"os:allow-family",
		"os:allow-arch",
		"os:allow-exe-extension",
		"os:allow-locale",
		"os:allow-hostname",
		"process:allow-restart",
		"fs:default",
		"os:default",
		"http:default",
		"process:default",
		"dialog:default",
		"shell:default"
	]
}
